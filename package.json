{
  "name": "eslint-config-current-thing",
  "version": "1.0.1",
  "description": "Lint The Current Thing",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "current": "npx eslint --print-config src/generate.ts > current.json",
    "lint": "npx eslint --fix -c ./eslint.config.js --max-warnings 0 .",
    "generate": "node --loader ts-node/esm --experimental-specifier-resolution=node --no-warnings ./src/generate.ts && npm run current && npx eslint --fix src/config.js"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/eslint-plugin": "^7.19.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^3.2.1",
